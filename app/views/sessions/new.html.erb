<h1 class='text-center'>Login</h1>
<div class='row'>
  <div class='col-md-6 offset-3'>
  <%= form_with(url: login_path, local:true) do |f| %>
   <%= f.label :name%>
   <%= f.text_field :name, class: 'form-control' %>
   <%= f.submit "Log in", class: 'mt-2 btn btn-primary'%>
   <% end%>
  </dvi>
</dvi>


